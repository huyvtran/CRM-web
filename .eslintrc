{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "$": true,
    "define": true,
    "require": true,
    "FC_CRM_USERID": true,
    "fc_hasFieldPrivilege": true,
    "fc_hasObjectPrivilege": true
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "amd": true
  },
  "rules": {
    "comma-dangle": 0,
    "one-var": 0,
    "no-unused-vars": 1,
    "max-len": [0, { "code": 120 }],
    "semi": [0, "always"],
    "generator-star-spacing": [0],
    "consistent-return": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "global-require": [1],
    "import/prefer-default-export": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "no-new-func": 1,
    "react/no-did-mount-set-state": 1,
    "no-shadow": 1,
    "no-param-reassign": 1,
    "react/no-unused-prop-types": 1,
    "react/prefer-stateless-function": [0],
    "no-else-return": [0],
    "no-restricted-syntax": [0],
    "react/jsx-no-target-blank": 1,
    "import/no-extraneous-dependencies": [0],
    "no-use-before-define": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-nested-ternary": [0],
    "arrow-body-style": [0],
    "import/extensions": [0],
    "no-bitwise": [0],
    "no-cond-assign": [0],
    "import/no-unresolved": [0],
    "require-yield": [1],
    "camelcase": "off",
    "linebreak-style": [0],
    "react/sort-comp": 1,
    "arrow-parens": [2, "always"],
    "no-lonely-if": 1,
    "array-callback-return": [1],
    "no-underscore-dangle": [1],
    "no-prototype-builtins": [1],
    "no-script-url": 1,
    "jsx-a11y/label-has-for": 1,
    "no-template-curly-in-string": 1,
    "react/no-string-refs": 1,
    "no-continue": 1,
    "no-useless-escape": 1,
    "no-plusplus": 1,
    "no-mixed-operators": 1,
    "react/react-in-jsx-scope": 1,
    "indent": 1,
    "radix": 1,
    "eqeqeq": 1,
    "react/jsx-indent": 1,
    "default-case": 1,
    "no-case-declarations": 1,
    "prefer-template": 1,
    "no-fallthrough": 1,
    "react/self-closing-comp": 1,
    "no-empty": 1
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
